{
  "Serilog": {
    "Using":  [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error"
      }
    },
    "WriteTo": [ 
      { 
        "Name": "Console", 
        "Args": {
          "outputTemplate": "{SourceContext}: {Message:lj}{NewLine}{Exception}"
        }
      } 
    ]
  }
}